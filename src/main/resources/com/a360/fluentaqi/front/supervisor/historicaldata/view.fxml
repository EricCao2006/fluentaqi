<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox styleClass="styled-container" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.a360.fluentaqi.front.supervisor.historicaldata.HistoricalDataController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label styleClass="styled-title" text="AQI反馈历史记录" />
    <ScrollPane hbarPolicy="ALWAYS" styleClass="styled-scroll-pane">
        <VBox.margin>
            <Insets left="20.0" right="20.0" />
        </VBox.margin>
        <TableView fx:id="txt_tableView" styleClass="styled-table" />
    </ScrollPane>
   <HBox alignment="CENTER" spacing="40.0">
      <children>
          <Button mnemonicParsing="false" onAction="#getback" styleClass="B-button" text="&lt;返回" />
          <Button fx:id="btnSettings" mnemonicParsing="false" onAction="#showSettingsMenu" styleClass="B-button">
              <graphic>
                  <ImageView fitHeight="24" fitWidth="24">
                      <Image url="@../../icons/settings.png" />
                  </ImageView>
              </graphic>
          </Button>
      </children>
   </HBox>
</VBox>
