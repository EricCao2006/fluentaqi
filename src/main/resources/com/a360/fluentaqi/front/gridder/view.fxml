<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox styleClass="styled-container" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.a360.fluentaqi.front.gridder.GridderController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label styleClass="styled-title" text="AQI反馈数据实测信息"/>
    <ScrollPane hbarPolicy="ALWAYS" styleClass="styled-scroll-pane">
        <VBox.margin>
            <Insets left="20.0" right="20.0"/>
        </VBox.margin>
        <TableView fx:id="txt_tableView" styleClass="styled-table">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </ScrollPane>
    <HBox alignment="CENTER" spacing="40.0">
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </VBox.margin>
        <VBox styleClass="styled-v-box">
            <HBox spacing="20.0" styleClass="styled-h-box">
                <padding>
                    <Insets left="20.0" right="20.0"/>
                </padding>
                <Label styleClass="styled-content" text="AQI反馈数据编号："/>
                <TextField fx:id="txt_afId" styleClass="styled-text-field"/>
            </HBox>
            <GridPane alignment="CENTER" hgap="10.0"
                      style="-fx-background-color: transparent; -fx-background-radius: 20; -fx-border-color: trabsparent; -fx-border-radius: 20;"
                      vgap="10.0">
                <columnConstraints>
                    <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="80.0"/>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="120.0"/>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="90.0"/>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="90.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints vgrow="SOMETIMES"/>
                    <RowConstraints vgrow="SOMETIMES"/>
                    <RowConstraints vgrow="SOMETIMES"/>
                </rowConstraints>
                <padding>
                    <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
                </padding>
                <Label styleClass="styled-small-content" text="SO2" GridPane.rowIndex="1"/>
                <Label styleClass="styled-small-content" text="CO" GridPane.rowIndex="2"/>
                <Label styleClass="styled-small-content" text="PM2.5" GridPane.rowIndex="3"/>
                <Label styleClass="styled-small-content" text="浓度等级" GridPane.columnIndex="2"/>
                <Label fx:id="label_so2level" styleClass="styled-small-content" text="？级"
                       GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <Label fx:id="label_colevel" styleClass="styled-small-content" text="？级"
                       GridPane.columnIndex="2" GridPane.rowIndex="2"/>
                <Label fx:id="label_pmlevel" styleClass="styled-small-content" text="？级"
                       GridPane.columnIndex="2" GridPane.rowIndex="3"/>
                <TextField fx:id="txt_so2" styleClass="styled-text-field" GridPane.columnIndex="1"
                           GridPane.rowIndex="1"/>
                <TextField fx:id="txt_co" styleClass="styled-text-field" GridPane.columnIndex="1"
                           GridPane.rowIndex="2"/>
                <TextField fx:id="txt_pm" styleClass="styled-text-field" GridPane.columnIndex="1"
                           GridPane.rowIndex="3"/>
                <Label styleClass="styled-small-content" text="污染物"/>
                <Label styleClass="styled-small-content" text="浓度(ug/m3)" GridPane.columnIndex="1"/>
                <Label styleClass="styled-small-content" text="污染等级" GridPane.columnIndex="3"/>
                <Label fx:id="label_so2explain" styleClass="styled-small-content" text="？？污染"
                       GridPane.columnIndex="3" GridPane.rowIndex="1"/>
                <Label fx:id="label_coexplain" styleClass="styled-small-content" text="？？污染"
                       GridPane.columnIndex="3" GridPane.rowIndex="2"/>
                <Label fx:id="label_pmexplain" styleClass="styled-small-content" text="？？污染"
                       GridPane.columnIndex="3" GridPane.rowIndex="3"/>
            </GridPane>
            <HBox spacing="40.0" styleClass="styled-h-box">
                <padding>
                    <Insets left="20.0" right="20.0"/>
                </padding>
                <Label styleClass="styled-small-content" text="实测AQI综合等级："/>
                <Label fx:id="label_confirmlevel" styleClass="styled-small-content" text="？级"/>
                <Label fx:id="label_confirmexplain" styleClass="styled-small-content" text="？？污染"/>
            </HBox>
        </VBox>
        <VBox alignment="CENTER" spacing="20.0">
            <Label styleClass="styled-small-content" text="网格员：">
                <padding>
                    <Insets bottom="-10.0"/>
                </padding>
            </Label>
            <VBox minWidth="120.0" spacing="5.0" styleClass="styled-v-box">
                <padding>
                    <Insets bottom="5.0" top="5.0"/>
                </padding>
                <VBox.margin>
                    <Insets bottom="-10.0" top="-5.0"/>
                </VBox.margin>
                <Label fx:id="label_realName" styleClass="styled-small-content" text="某某某"/>
            </VBox>
            <Button minWidth="120.0" mnemonicParsing="false" onAction="#confirmData" styleClass="A-button"
                    text="提交&gt;"/>
            <Button minWidth="120.0" mnemonicParsing="false" onAction="#getback" styleClass="B-button"
                    text="&lt;返回"/>
        </VBox>
    </HBox>

</VBox>
