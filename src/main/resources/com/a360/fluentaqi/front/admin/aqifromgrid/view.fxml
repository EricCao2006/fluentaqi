<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<VBox styleClass="styled-container" xmlns="http://javafx.com/javafx/17.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.a360.fluentaqi.front.admin.aqifromgrid.AqiFromGridController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label styleClass="styled-title" text="网格员AQI实测数据"/>
    <ScrollPane styleClass="styled-scroll-pane" hbarPolicy="ALWAYS">
        <VBox.margin>
            <Insets left="20.0" right="20.0"/>
        </VBox.margin>
        <TableView fx:id="txt_tableView" styleClass="styled-table"/>
    </ScrollPane>
    <Button mnemonicParsing="false" onAction="#getback" styleClass="B-button" text="&lt;返回"/>
</VBox>
